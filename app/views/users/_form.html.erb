<%= form_for @user do |f| %>
<div class="inputs">
  <%=f.label :username%>
  <%=f.text_field :username%>
</div>
<div class="inputs">
  <%=f.label :password%>
  <%=f.password_field :password%>
</div>
<div class="inputs">
  <%=f.label :age%>
  <%=f.text_field :age%>
</div>
<div class="inputs">
  <%=f.label :image_url%>
  <%=f.text_field :image_url%>
</div>
<div class="inputs">
  <%=f.label :city%>
  <%=f.text_field :city%>
</div>
<div class="inputs">
  <%=f.label :state%>
  <%=f.text_field :state%>
</div>
<div class="bio">
  <%=f.label :bio%>
  <%=f.text_field :bio%>
</div>
  <%=f.submit 'Submit', class: 'btn form-submit'%>
<% end%>
